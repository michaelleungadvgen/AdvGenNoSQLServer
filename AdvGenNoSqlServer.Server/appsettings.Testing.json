{
  "Server": {
    "Host": "127.0.0.1",
    "Port": 19090,
    "MaxConnections": 50,
    "ConnectionTimeout": 10000,
    "KeepAliveInterval": 30000,
    "ReceiveBufferSize": 32768,
    "SendBufferSize": 32768
  },
  "Security": {
    "EnableSSL": false,
    "CertificatePath": "",
    "CertificatePassword": "",
    "RequireAuthentication": false,
    "AllowedAuthMethods": ["Token"],
    "SessionTimeout": 600000,
    "MaxFailedAttempts": 10,
    "LockoutDuration": 60000
  },
  "Storage": {
    "DataPath": "./test-data",
    "MaxFileSize": 104857600,
    "CompressionEnabled": false,
    "CompressionLevel": "fastest",
    "PersistenceMode": "async",
    "IndexCacheSize": 67108864
  },
  "Transaction": {
    "DefaultIsolationLevel": "ReadCommitted",
    "DefaultTimeout": 10000,
    "MaxConcurrentTransactions": 100,
    "DeadlockDetection": true,
    "EnableWAL": true,
    "WALFlushInterval": 100
  },
  "Cache": {
    "Enabled": true,
    "MaxSize": 67108864,
    "MaxItemCount": 1000,
    "EvictionPolicy": "LRU",
    "DefaultTTL": 60000,
    "EnableDistributed": false
  },
  "Logging": {
    "Level": "Debug",
    "EnableFileLogging": false,
    "LogPath": "./test-logs",
    "MaxLogFileSize": 10485760,
    "MaxLogFiles": 5,
    "EnableConsoleLogging": true,
    "EnableAuditLogging": false
  },
  "Performance": {
    "MaxQueryTimeout": 10000,
    "EnableQueryOptimization": false,
    "BatchSize": 100,
    "ThreadPoolMinThreads": 2,
    "ThreadPoolMaxThreads": 16,
    "GCMode": "Workstation"
  }
}
